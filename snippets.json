{
   "req struct" : {
      "body" : [
         "${1:req} ${2:num} {",
         "   request {",
         "       ${3:req}",
         "   }",
         "   response {",
         "       r 0 : integer",
         "       ${4:resp}",
         "   }",
         "}"
      ],
      "description" : "通常req",
      "prefix" : "st"
   },
   "resp item struct" : {
      "body" : [
         "${1:req} ${2:num} {",
         "   request {",
         "       ${3:req}",
         "   }",
         "   response {",
         "       r 0 : integer",
         "       items 1 : *item(id)",
         "       ${3:resp}",
         "   }",
         "}"
      ],
      "description" : "返回items的req",
      "prefix" : "sti"
   },
   "sync struct" : {
      "body" : [
         "sync_${1:req} ${2:num} {",
         "   request {",
         "       ${3:struct}",
         "   }",
         "}"
      ],
      "description" : "sync消息的结构",
      "prefix" : "sync"
   }
}
